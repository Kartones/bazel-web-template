{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "declaration": true,
    // "baseUrl": ".",
    "types": ["node"],
    "incremental": true,
    "removeComments": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": true
  }
}